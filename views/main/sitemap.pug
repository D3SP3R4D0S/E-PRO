head
    include compara/head.pug
body(id="page-top")
    div(id="wrapper")
        include compara/sidebar.pug
        div(id="content-wrapper" class="d-flex flex-column")
            div(id="content")
                include compara/topbar.pug
                div(class='container-fluid')
                    H2(class="m-4 text-dark") Site map
                    div(class="col-lg-12")
                        div(class='card shadow')
                            div(class='card-body')
                                table(class="table")
                                    thead
                                        tr
                                            th Title
                                            th Link
                                    tbody
                                        tr
                                            td sample
                                            td
                                                a(href="#") sample

            include compara/footer.pug
    include compara/footer_script.pug